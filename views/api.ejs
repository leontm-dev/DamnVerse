                        <div class="element-name">/instagram/externer_link</div>
                    </div>
                    <div class="sidebar-element">
                        <div class="element-type-get">GET</div>
                        <div class="element-name">/instagram/biographie</div>
                    </div>
                    <div class="sidebar-element">
                        <div class="element-type-get">GET</div>
                        <div class="element-name">/instagram/kategorie</div>
                    </div>
                    <div class="sidebar-element">
                        <div class="element-type-get">GET</div>
                        <div class="element-name">/instagram/follower</div>
                    </div>
                    <div class="sidebar-element">
                        <div class="element-type-get">GET</div>
                        <div class="element-name">/twitch/profil</div>
                    </div>
                    <div class="sidebar-element">
                        <div class="element-type-get">GET</div>
                        <div class="element-name">/twitch/follower</div>
                    </div>
                    <div class="sidebar-element">
                        <div class="element-type-get">GET</div>
                        <div class="element-name">/twitch/name</div>
                    </div>
                    <div class="sidebar-element">
                        <div class="element-type-get">GET</div>
                        <div class="element-name">/twitch/beschreibung</div>
                    </div>
                    <div class="sidebar-element">
                        <div class="element-type-get">GET</div>
                        <div class="element-name">/twitch/aufrufe</div>
                    </div>
                    <div class="sidebar-element" onclick="">
                        <div class="element-type-get">GET</div>
                        <div class="element-name">/twitch/nachrichten?name=username</div>
                    </div>
                </div>
            </div>
            <div class="code-field" id="code-field"></div>
        </div>
    </div>
    <script src="/Api-Lander/script.js"></script>
    <script src="/Api-Lander/particles.js"></script>
    <script src="/Api-Lander/app.js"></script>
</body>
</html>